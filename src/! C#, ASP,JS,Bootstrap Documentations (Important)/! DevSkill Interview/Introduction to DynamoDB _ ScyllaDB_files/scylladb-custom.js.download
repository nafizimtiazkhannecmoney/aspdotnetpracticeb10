  function checkSeshDesc() {
    var sessionizeLoader = $(
      ".summit-agenda.y2025 .sessionize-loader .sz-session__description"
    );

    if (sessionizeLoader.length) {
      var agendaTitle = $(document).find(".sz-session__title");
